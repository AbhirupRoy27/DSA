int main()
{
int i;
int n;
int k;
cin>>n;
int a[n];
for(i=0;i<n;i++)
    {
    cin>>a[i];
    }
    cin>>k;
    for(int i=k;i<n;i++)
    {
        a[i]=a[i+1];
    }
    for(i =0;i<n-1;i++)
    cout<<a[i];  
    return 0;
}

