#include <iostream>
using namespace std;
int main() {
    int t;
    cin>>t;
    while(t--) {
        int max,a[6];
        for(int i=0;i<6;i++) {
            cin>>a[i];
        }
        max=a[0];
        for(int i=0;i<6;i++) {
            if(max<a[i]) {
                max=a[i];
            }
        }
        cout<<max<<endl;
    }
    return 0;
}
